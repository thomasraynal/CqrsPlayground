{
  "spring": {
    "application": {
      "name": "trades"
    }
  },
  "identity": {
    "key": "cqrsplayground.trade.service",
    "token": "__cqrsplayground.trade.service.token__"
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8080/eureka/v2",
      "shouldRegisterWithEureka": true,
      "shouldFetchRegistry": false,
      "validate_certificates": false
    },
    "instance": {
      "appname": "trades",
      "nonSecurePort": 5000,
      "port": 5000,
      "instanceId": "localhost:trades:5000",
      "leaseRenewalIntervalInSeconds": 5,
      "leaseExpirationDurationInSeconds": 10
    }
  }
}